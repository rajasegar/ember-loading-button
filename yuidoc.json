{
  "name": "ember-loading-button",
  "description": "Ember async buttons with loading indicators",
  "url": "https://github.com/rajasegar/ember-loading-button",
  "version": "0.1.2",
  "options": {
    "paths": [
      "addon"
    ],
    "exclude": "vendor",
    "outdir": "docs",
    "linkNatives": true,
    "quiet": true,
    "parseOnly": false,
    "lint": false,
    "enabledEnvironments": ["development","production"],
    "themedir": "node_modules/yuidoc-ember-theme",
    "helpers": ["node_modules/yuidoc-ember-theme/helpers/helpers.js"]
  }
}
